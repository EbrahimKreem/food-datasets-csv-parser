{
  "name": "@groceristar/food-dataset-csv-parser",
  "version": "0.1.10",
  "license": "MIT",
  "scripts": {
    "bundle": "rollup -c",
    "dev": "rollup -c -w",
    "build": "npm run clean && npm run bundle",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "eslint ./src --fix",
    "format": "prettier",
    "play": "node ./src/play/play.js",
    "build:babel": "yarn clean && babel src -d dist",
    "parse:FoodComposition": "node ./runner/parseFoodComposition",
    "debug:FoodComposition": "npm run build && node ./debug/parseFoodComposition",
    "csv:fc": "node ./examples/projects/FoodComposition/csv_parser",
    "csv:usfa1": "node ./examples/projects/USFA/Derivation_Code_Description/parser",
    "csv:usfa2": "node ./examples/projects/USFA/Nutrition/parser",
    "csv:usfa3": "node ./examples/projects/USFA/Product/parser",
    "csv:usfa4": "node ./examples/projects/USFA/Serving_Size/parser"
  },
  "description": "module to use static JSON files with groceristar projects",
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "browser": "dist/index.iife.js",
  "repository": "https://github.com/GroceriStar/food-datasets-csv-parser",
  "author": "Arthur Tkachenko",
  "private": false,
  "release": {
    "publishDir": "dist"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-plugin-module-resolver": "3.2.0",
    "coveralls": "3.0.5",
    "eslint": "6.0.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.11.1",
    "husky": "3.0.1",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "regenerator-runtime": "0.13.3",
    "rollup": "1.17.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "shx": "0.3.2"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@groceristar/static-data-generator": "0.1.3",
    "csv-parser": "^2.3.0",
    "fs": "^0.0.2",
    "lodash": "^4.17.11",
    "path": "^0.12.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run clean",
      "after": "lint-staged",
      "then": "yarn precommit"
    }
  },
  "files": [
    "dist"
  ]
}
